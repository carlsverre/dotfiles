{
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  },
  "python.jediEnabled": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "rust",
    "go"
  ],
  "tsserver.formatOnType": true,
  "coc.preferences.formatOnType": true,
  "inlayHint.enable": false
}
